{
  "name": "object-pick",
  "version": "1.0.0",
  "description": "Typescript version for _.pick from lodash. Returns filtered object based on keys in arguments",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delprzemo/object-pick.git"
  },
  "keywords": ["object", "pick", "typescript", "filter"],
  "author": "delprzemo",
  "license": "ISC",
  "files": ["lib/**/*"],
  "bugs": {
    "url": "https://github.com/delprzemo/object-pick/issues"
  },
  "homepage": "https://github.com/delprzemo/object-pick#readme",
  "devDependencies": {
    "@types/node": "^9.4.6",
    "object.pick": "^1.3.0",
    "prettier": "^1.16.4",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  }
}
